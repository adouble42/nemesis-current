	      FIPS202_SHAKE256(shake_outp, NTRU_PRIVLEN, (uint8_t *)shake_finalp, NTRU_PRIVLEN);
	      FIPS202_SHAKE256(shake_finalp, NTRU_PRIVLEN, (uint8_t *)shake_outp, NTRU_PRIVLEN);
	    }

 	    /* generate an NTRU key */
